# Database Configuration
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control!

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=dashboard
POSTGRES_PORT=5432

# Application Database Connection
# If not set, will use POSTGRES_* values above
DB_HOST=postgres
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=dashboard

# SSL Mode
# disable - для локальной разработки (миграции НЕ выполняются автоматически)
# require/verify-full - для production (миграции выполняются автоматически)
DB_SSLMODE=disable

# Backend Port
BACKEND_PORT=3000

